<h1>LOGIN PAGE!</h1>
<%= flash[:error_message] %>
<%= form_for :login do |f|%>
    <p><%= f.label :username%>
    <%= f.text_field :username%></p>
    <%= f.label :password%>
    <%= f.password_field :password%><br>
    <input class="btn btn-outline-secondary" type="submit" value="LOG IN">

    <p>New user? <%= link_to 'Sign Up Here', signup_path %></p>
    
    <%end%>

    